<mapper namespace="com.gary.cloudinteractive.webapi.mapper.ZipCodeMapper" >

    <sql id="Base_Column_List" >
        id, name
    </sql>

    <select id="getAll" resultType="hashmap" >
        SELECT
        <include refid="Base_Column_List" />
        FROM ZIP_CODE
    </select>

    <select id="getAllModel" resultType="zipCode" >
        SELECT
        <include refid="Base_Column_List" />
        FROM ZIP_CODE
    </select>

    <select id="getOne" parameterType="int" resultType="hashmap" >
        SELECT
        <include refid="Base_Column_List" />
        FROM ZIP_CODE
        WHERE id = #{id}
    </select>

